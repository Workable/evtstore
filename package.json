  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seikho/evtstore/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/dotenv": "^6.1.1",
    "@types/mocha": "^5.2.7",
    "@types/pg": "^8.6.1",
    "@types/sqlite3": "^3.1.5",
    "chai": "^4.2.0",
    "dotenv": "^8.1.0",
    "knex": "^2.4.0",
    "mocha": "^10.2.0",
    "mongodb": "^4.11.0",
    "neo4j-driver": "^4.1.2",
    "pg": "^8.2.1",
    "postgres": "^3.2.4",
    "rimraf": "^3.0.0",
    "sqlite3": "^5.1.4",
    "typescript": "^4.9.4"
  }
